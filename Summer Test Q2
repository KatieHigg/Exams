#Student Name:Katie Higgins

import random


num=random.randint(num >=1 and num <=50)

guess=int("Please guess a number between 1 and 50: ")

while guess != num:
    if (guess < num):
        print("Too low. Guess again.")
    elif(guess > num):
        print("Too high. Guess again.")
    else:
        print("You win!!")
        break
